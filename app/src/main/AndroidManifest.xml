<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shichen.mobapisample">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".config.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:vmSafeMode="true">
        <activity android:name=".config.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".weatherpart.WeatherInfoActivity"
            android:launchMode="standard" />
        <activity
            android:name=".weatherpart.PickTargetCityActivity"
            android:launchMode="standard" />
        <activity
            android:name=".weatherpart.AirConditionActivity"
            android:launchMode="standard" />
        <activity
            android:name=".cookbookpart.CookBookMenuActivity"
            android:launchMode="standard" />
        <activity
            android:name=".cookbookpart.ViewCookBookActivity"
            android:launchMode="standard" />
    </application>

</manifest>